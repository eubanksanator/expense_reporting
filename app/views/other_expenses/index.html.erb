<p id="notice"><%= notice %></p>

<h1>Listing Other Expenses</h1>

<table>
  <thead>
    <tr>
      <th>Occurence</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_expenses.each do |other_expense| %>
      <tr>
        <td><%= other_expense.occurence %></td>
        <td><%= other_expense.description %></td>
        <td><%= link_to 'Show', other_expense %></td>
        <td><%= link_to 'Edit', edit_other_expense_path(other_expense) %></td>
        <td><%= link_to 'Destroy', other_expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other expense', new_other_expense_path %>
